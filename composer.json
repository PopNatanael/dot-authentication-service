{
  "name": "dotkernel/dot-authentication-service",
  "type": "library",
  "description": "Concrete implementation of an AuthenticationInterface based on Zend Authentication",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "psr/http-message": "^1.0",
    "container-interop/container-interop": "^1.1",
    "zendframework/zend-authentication": "^2.5",
    "zendframework/zend-psr7bridge": "^0.2",

    "dotkernel/dot-authentication": "0.6.x-dev",
    "zendframework/zend-servicemanager": "^3.1",
    "zendframework/zend-hydrator": "^2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-session": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Authentication\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Authentication\\": "test/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/dotkernel/dot-authentication.git"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.6-dev"
    }
  }
}
